{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "require": {
        "php": "^7.3|^8.0",
        "eluceo/ical": "^2.4",
        "fideloper/proxy": "^4.4",
        "fruitcake/laravel-cors": "^2.0",
        "guzzlehttp/guzzle": "^7.0.1",
        "intervention/image": "^2.7",
        "intervention/imagecache": "^2.5",
        "laravel/fortify": "^1.8",
        "laravel/framework": "^8.75.0",
        "laravel/nova": "^3",
        "laravel/tinker": "^2",
        "spatie/laravel-activitylog": "^4.3",
        "spatie/laravel-cookie-consent": "^3.1",
        "spatie/laravel-tags": "^4.2"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.6",
        "beyondcode/laravel-query-detector": "^1.5",
        "facade/ignition": "^2.5",
        "fakerphp/faker": "^1",
        "laravel/dusk": "^6",
        "laravel/sail": "^1",
        "mockery/mockery": "^1.4",
        "nunomaduro/collision": "^5",
        "phpunit/phpunit": "^9.5"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/",
            "Talk\\": "modules/talk/src/",
            "Talk\\Database\\Factories\\": "modules/talk/database/factories",
            "Matcher\\": "modules/matcher/src/",
            "Matcher\\Database\\Factories\\": "modules/matcher/database/factories",
            "Matcher\\Database\\Seeders\\": "modules/matcher/database/seeders"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "post-update-cmd": [
            "@php artisan nova:publish"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "repositories": {
        "nova": {
            "type": "composer",
            "url": "https://nova.laravel.com"
        }
    }
}
